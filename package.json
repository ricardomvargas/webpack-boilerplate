{
    "name": "webpackboilerplate",
    "version": "1.0.0",
    "author": "Ricardo Moura Vargas",
    "license": "ISC",
    "description": "Base web project to create applications with webpack and React.",
    "main": "index.js",
    "scripts": {
        "dev": "webpack serve",
        "build": "webpack",
        "test": "jest --watch"
    },
    "keywords": [
        "webpack",
        "react",
        "boilerplate "
    ],
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "testEnvironment": "jsdom"
    },
    "dependencies": {
        "@types/jest": "^28.1.3",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.8",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@babel/runtime": "^7.17.8",
        "@testing-library/react": "^13.3.0",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "babel-loader": "^8.2.4",
        "css-loader": "^6.7.1",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^28.1.1",
        "jest-environment-jsdom": "^28.1.1",
        "style-loader": "^3.3.1",
        "typescript": "^4.6.3",
        "webpack": "^5.71.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.0"
    }
}
